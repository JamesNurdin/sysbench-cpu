{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "nodes": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "RUN_NAME": {
      "type": "string"
    },
    "resources": {
      "type": "object",
      "properties": {
        "cpu": {
          "type": "string"
        },
        "memory": {
          "type": "string"
        }
      },
      "required": [
        "cpu",
        "memory"
      ]
    },
    "iterations": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "duration": {
      "type": "integer"
    }
  },
  "required": [
    "RUN_NAME",
    "duration",
    "iterations",
    "nodes",
    "resources"
  ]
}